//Write a program that swaps the values of two integers using pointers.
#include <stdio.h>
  int addsub(int a, int b){
    int add, sub;
    add = a + b;
    sub = a - b;
    a = add;
    b = sub;
    printf("a : %d and b : %d", a, b);
    return 0;
  }
int main(void){
  int a = 20, b = 30;
  
  addsub(a, b);
  
  return 0;
}
